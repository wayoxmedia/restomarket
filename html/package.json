{
  "name": "template1",
  "description": "Include Bootstrap's source Sass and some JavaScript plugins.",
  "version": "1.0.0",
  "private": true,
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "compile-sass": "npx sass --watch assets/scss/styles.scss assets/css/styles.min.css",
    "build:css": "sass static/admin/scss/styles.scss:static/admin/css/styles.min.css --style=compressed --source-map --load-path=node_modules",
    "watch:css": "sass --watch static/admin/scss/styles.scss:static/admin/css/styles.min.css --style=compressed --source-map --load-path=node_modules"
  },
  "devDependencies": {
    "sass": "^1.54.8"
  },
  "dependencies": {
    "apexcharts": "^3.54.1",
    "bootstrap": "^5.3.6"
  }
}
