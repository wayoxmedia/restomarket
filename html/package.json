{
  "name": "eglee-gourmet",
  "description": "Include Bootstrap's source Sass and individual JavaScript plugins.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "sass static/admin/scss/styles.scss:static/admin/css/styles.min.css --style=compressed --source-map --load-path=node_modules",
